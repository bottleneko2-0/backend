generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model users_test {
  id       Int    @unique(map: "id_UNIQUE") @default(autoincrement())
  username String @db.VarChar(50)
  email    String @db.VarChar(100)
  password String @db.VarChar(255)
}
